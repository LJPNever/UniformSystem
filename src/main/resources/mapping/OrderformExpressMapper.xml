<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="yidong.mapper.OrderformExpressMapper" >

  <select id="selectByOrderformId" resultType="Express">
    SELECT * FROM tb_express where orderform_id=#{id}
  </select>
  <insert id="addExpress" parameterType="Express">
    INSERT INTO tb_express(delivery,expressNumber,expressCompany,orderform_id,goods_name,goods_modle,goods_num,goods_price)VALUE
    (#{delivery},#{expressNumber},#{expressCompany},#{orderform_id},#{goods_name},#{goods_modle},#{goods_num},#{goods_price})
  </insert>
</mapper>