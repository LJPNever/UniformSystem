<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="yidong.mapper.FreeshippingMapper" >
  <update id="setFree" parameterType="Freeshipping">
    UPDATE tb_freeshipping set freeConditions=#{freeConditions},free=#{free}
  </update>

  <select id="getFree" resultType="Freeshipping">
    SELECT * FROM tb_freeshipping
  </select>
</mapper>